# TensorFlow Serving 模型配置文件
# 
# 支持多模型部署和版本管理
# 详细文档: https://www.tensorflow.org/tfx/serving/serving_config

model_config_list {
  config {
    name: "wide_deep_ctr"
    base_path: "/models/wide_deep_ctr"
    model_platform: "tensorflow"
    
    # 模型版本策略
    model_version_policy {
      # 加载所有可用版本
      all {}
      
      # 或者只加载最新版本:
      # latest {
      #   num_versions: 2
      # }
      
      # 或者加载指定版本:
      # specific {
      #   versions: 1
      #   versions: 2
      # }
    }
    
    # 版本标签（可选）
    # 允许使用标签而不是版本号来请求模型
    # version_labels {
    #   key: "stable"
    #   value: 1
    # }
    # version_labels {
    #   key: "canary"
    #   value: 2
    # }
  }
  
  # 可以添加更多模型配置
  # config {
  #   name: "another_model"
  #   base_path: "/models/another_model"
  #   model_platform: "tensorflow"
  # }
}
